<%= render :partial => 'shared/errors', :locals => {:object => @artist} %>

<%= form_for @artist do |f| %> 
     <div class = 'field'>
     	<%= f.label :name %>
     	<%= f.text_field :name %>
     </div>
     <div class = 'field'>
     	<%= f.label :photo %><br />
	    <%= f.file_field :photo %>
	    <%= f.hidden_field :photo_cache %>
	    <%= f.text_field :remote_photo_url %>

     </div>

     
     
     <div class = 'field'>
     	<%= f.submit %>
     </div>
<% end %>

<br>
